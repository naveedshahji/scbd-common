{

  "name": "@scbd/common",
  "private": false,
  "version": "0.1.1",
 
  "repository": {
    "type": "git",
    "url": "https://github.com/scbd/scbd-common.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "browser": "dist/index.umd.js",

  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.umd.js"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "require": "./dist/components/*.cjs",
      "default": "./dist/components/*.umd.js"
    },
    "./services/*": {
      "import": "./dist/services/*.js",
      "require": "./dist/services/*.cjs",
      "default": "./dist/services/*.umd.js"
    }
  },
  "files": [
    "dist/*",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist && rollup -c",
    "build:watch": "rm -rf ./dist && rollup -c --watch",
    "cleanup": "rm -rf ./dist && rm -rf ./node_modules",
    "cleanup:hard": "rm -rf ./dist && rm -rf ./node_modules && rm -rf ./package-lock.json"
  },
  
  "name": "cbd-common",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "@vue/compat": "^3.4.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.70.0",
    "vite": "^5.0.11"
  }
}
